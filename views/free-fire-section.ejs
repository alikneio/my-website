                        <img src=<%= product.image %>" class="card-img-top p-3" alt="<%= product.name %>" style="max-height: 150px; object-fit: contain;">
                        <div class="card-body d-flex flex-column">
                            <h5 class="card-title"><%= product.name %></h5>
                            <p class="card-text fs-4 fw-bold mt-auto text-primary">$<%= product.price.toFixed(2) %></p>
                        </div>
                    </div>
                </a>
            </div>
        <% }) %>
    <% } else { %>
        <div class="col-12"><p class="text-muted fs-4 text-center">No products found.</p></div>
    <% } %>
</div>


